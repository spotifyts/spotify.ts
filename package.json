{
	"name": "spotify.ts",
	"version": "1.3.0",
	"description": "An easy to use, object oriented API wrapper for the Spotify Web API.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": "https://github.com/spotifyts/spotify.ts",
	"author": "FC5570",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "yarn tsc -b src && yarn gen-esm",
		"format": "yarn prettier --write {src,tests}/**/*.ts",
		"test": "yarn vitest",
		"prepublishOnly": "yarn build && yarn format",
		"lint": "yarn eslint src tests --ext ts --fix",
		"gen-esm": "yarn gen-esm-wrapper dist/index.js dist/index.mjs",
		"prepare": "yarn husky install"
	},
	"dependencies": {
		"@discordjs/collection": "^1.5.1",
		"@sapphire/async-queue": "^1.5.0",
		"phin": "^3.7.0",
		"tslib": "^2.5.0",
		"vitest": "^0.30.1"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.6.1",
		"@sapphire/eslint-config": "^4.4.2",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/eslint": "^8.37.0",
		"@types/node": "^18.16.3",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"commitlint": "^17.6.1",
		"dotenv": "^16.0.3",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.5.0",
		"typedoc": "^0.24.6",
		"typescript": "^4.9.5"
	},
	"packageManager": "yarn@3.5.1",
	"prettier": "@sapphire/prettier-config",
	"files": [
		"dist/",
		"!dist.tsconfig.tsbuildinfo"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.ts": "yarn eslint --fix"
	}
}
