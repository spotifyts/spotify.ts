{
	"name": "spotify.ts",
	"version": "1.0.0",
	"description": "An easy to use, object oriented API wrapper for the Spotify Web API.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": "https://github.com/spotifyts/spotify.ts",
	"author": "FC5570",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "yarn tsup src && yarn gen-esm",
		"format": "yarn prettier --write {src,tests}/**/*.ts",
		"test": "yarn jest",
		"lint": "yarn eslint src tests --ext ts --fix",
		"gen-esm": "yarn gen-esm-wrapper dist/index.js dist/index.mjs",
		"prepare": "yarn husky install"
	},
	"dependencies": {
		"@discordjs/collection": "^0.5.0",
		"@sapphire/async-queue": "^1.2.0",
		"phin": "^3.6.1",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^16.2.1",
		"@sapphire/eslint-config": "^4.2.1",
		"@sapphire/prettier-config": "^1.3.0",
		"@sapphire/ts-config": "^3.3.1",
		"@types/eslint": "^8",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.12",
		"@types/prettier": "^2",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"commitlint": "^16.2.1",
		"dotenv": "^16.0.0",
		"eslint": "^8.9.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"jest-circus": "^27.5.1",
		"lint-staged": "^12.3.4",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.5.0",
		"tsup": "^5.11.13",
		"typedoc": "^0.22.12",
		"typescript": "^4.5.5"
	},
	"packageManager": "yarn@3.2.0",
	"prettier": "@sapphire/prettier-config",
	"files": [
		"dist/",
		"!dist.tsconfig.tsbuildinfo"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.ts": "yarn eslint --fix"
	}
}
